#include <stdio.h>

int main()
{
	int a1,a2,a3,b1,b2,b3,ar,br;
	ar = 0;
	br = 0;
	scanf("%d %d %d",&a1,&a2,&a3);
	scanf("%d %d %d",&b1,&b2,&b3);
	
	if(a1>b1)
		ar+=1;
    
	else if(b1>a1)
		br+=1;
		
	if(a2>b2)
		ar+=1;
		
	else if(b2>a2)
		br+=1;
				
	if(a3>b3)
		ar+=1;
		
	else if(b3>3)
		br+=1;
		
	printf("%d %d",ar,br);
	
	
	
	
	return 0;
}

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n,a1=0,a2=0,sum,j,i,b,c; 
    scanf("%d",&n);
    int a[n][n],a3[n][n];
    for(i = 0; i < n;i++){
       for(j = 0; j < n; j++){
          
          scanf("%d",&a[i][j]);
          
          
       }
    }
    
    for(b=0;b<n;b++){
    	a1=a1+a[b][b];

    }
    for(c=0;c<n;c++){
    	a2=a2+a[c][n-c-1];
    }
    printf("%d",abs(a1-a2));
   
    
    return 0;
}

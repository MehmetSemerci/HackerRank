#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int i,a,n,k,j,b[1001],d=0;
    scanf("%d",&a);
    
    for(i=0;i<a;i++)
    {
    	d=0;
    	scanf("%d %d",&n,&k);
    	
    	for(j=0;j<n;j++)
    	{
    		fflush(stdin);
    		scanf("%d",&b[j]);
    		if(b[j]<=0){
    		d++;	
    		}
    		else{
    		}
    	}
    	if(d>=k){
    		printf("NO\n");
    	}
    	else if (d<k){
    		printf("YES\n");
    	}
    	
    	
    	
    }
	
    return 0;
}

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n,i;
	  long long int number, sum=0; 
    scanf("%d",&n);
    int arr[n];
    for(i = 0; i < n; i++){
       scanf("%lli",&number);
       sum = sum + number;
    }
    printf("%lli",sum);
    return 0;
}

